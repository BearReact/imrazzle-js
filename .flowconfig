[ignore]
.*/test/.*

[untyped]
.*/node_modules/**/.*
.*/internals/flow/.*


[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import

[libs]
internals/flow/

[include]
./src/

[options]
include_warnings=true
suppress_comment= \\(.\\|\n\\)*\\$flow-ignore

module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.less
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper.extension='less' -> 'empty/object'

esproposal.decorators=ignore


;自定義路徑
module.name_mapper='^@static' ->'<PROJECT_ROOT>/static'
module.name_mapper='^@assets' ->'<PROJECT_ROOT>/src/resources/assets'
module.name_mapper='^@components' ->'<PROJECT_ROOT>/src/resources/components'
module.name_mapper='^@utils' ->'<PROJECT_ROOT>/src/utils'
module.name_mapper='^@services' ->'<PROJECT_ROOT>/src/services'
module.name_mapper='^@pages' ->'<PROJECT_ROOT>/src/pages'
module.name_mapper='^@layouts' ->'<PROJECT_ROOT>/src/layouts'
module.name_mapper='^@config' ->'<PROJECT_ROOT>/src/config'
module.name_mapper='^@middleware' ->'<PROJECT_ROOT>/src/middleware'
module.name_mapper='^@test' ->'<PROJECT_ROOT>/test'

;自定義路徑 > library
module.name_mapper='^@store' ->'<PROJECT_ROOT>/src/library/redux/store'
module.name_mapper='^@router' ->'<PROJECT_ROOT>/src/library/react-router'
module.name_mapper='^@styled-bs-grid' ->'<PROJECT_ROOT>/src/library/styled-bs-grid'
module.name_mapper='^@i18n' ->'<PROJECT_ROOT>/src/library/intl'

[version]
^0.118.0

{
  "name": "imezaale-js",
  "version": "0.1.1",
  "description": "A React framework for web artisans razzle.js",
  "author": "imagine10255",
  "license": "MIT",
  "engines": {
    "node": ">=13.5.0"
  },
  "scripts": {
    "dev": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node build/server.js",
    "test:mock-api": "mocker ./test/mocker-api",
    "test:prod": "yarn build && yarn start",
    "storybook": "start-storybook -p 6006 -c ./test/storybook",
    "lint": "eslint .",
    "lint:flow": "flow",
    "lint:fix": "eslint . --fix",
    "cz": "./node_modules/cz-customizable/standalone.js",
    "release": "standard-version -r patch",
    "release:minor": "standard-version -r minor",
    "release:major": "standard-version -r major",
    "release:pre": "standard-version --prerelease alpha",
    "release:push": "git push --follow-tags origin master"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "cz-customizable": {
      "config": "./internals/cz-customizable/config.js"
    }
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^1.5.2",
    "@formatjs/intl-relativetimeformat": "^4.5.9",
    "apisauce": "1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "classnames": "^2.2.6",
    "connected-react-router": "6.7.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.4.2",
    "dayjs": "^1.8.21",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "razzle": "3.0.0",
    "react": "16.13.0",
    "react-cookie": "^4.0.3",
    "react-dom": "16.13.0",
    "react-epic-spinners": "^0.4.1",
    "react-hook-form": "^4.9.8",
    "react-intl": "^3.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "reduxsauce": "1.1.2",
    "seamless-immutable": "7.1.4",
    "seamless-immutable-mergers": "7.1.0",
    "serialize-javascript": "^3.0.0",
    "styled-components": "5.0.1",
    "universal-cookie": "^4.0.3",
    "universal-cookie-express": "^4.0.3",
    "xmldom": "^0.2.1",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-backgrounds": "^5.3.14",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addon-notes": "5.3.14",
    "@storybook/addon-viewport": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/react": "5.3.14",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.6",
    "cz-conventional-changelog": "^3.1.0",
    "cz-customizable": "^6.2.0",
    "doctrine": "^3.0.0",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "flow-bin": "0.118.0",
    "flow-typed": "^3.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocker-api": "^1.12.2",
    "react-is": "^16.13.0",
    "require-context": "^1.1.0",
    "standard-version": "^7.1.0",
    "storybook-addon-i18n": "^5.1.13",
    "validate-commit-msg": "^2.14.0"
  }
}
